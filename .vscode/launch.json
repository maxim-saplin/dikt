{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Profile",
      "request": "launch",
      "type": "dart",
      "flutterMode" : "profile"
    },
    {
      "name": "Debug",
      "request": "launch",
      "type": "dart",
      "flutterMode" : "debug",
      "program" : "lib/main.dart",
      "toolArgs": [
        "--dart-define",
        "testVal=MY_VALUE",
      ]
    },
    {
      "name": "Debug (dartdefine)",
      "request": "launch",
      "type": "dart",
      "flutterMode" : "debug",
      "program" : "lib/main.dart",
      "args": [
        "--dart-define", "testVal=TestValue",
      ],
    },
    {
      "name": "Release",
      "request": "launch",
      "type": "dart",
      "flutterMode" : "release",
      "program" : "lib/main.dart"
    }
  ]
}